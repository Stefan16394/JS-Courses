<section id="{{viewName}}" class="{{viewName}}">
<h1 class="titleForm">{{viewMessage}}</h1>
<form id="{{formName}}" class="form">
    <div>Username:</div>
    <div><input type="text" name="username" required /></div>
    <div>Password:</div>
    <div><input type="password" name="passwd" required /></div>
    <div><input type="button" id="{{buttonName}}" value="{{buttonValue}}" /></div>
</form>
</section>
